        <arguments>
          <argument>
            <kind>1</kind>
            <string>//see obj_wolf

texture_set_interpolation(false);

//scr_enemyCollisionAndMove()
//wall

if (place_meeting(x, y+1, obj_enemy_barrier))
{
    vsp = -1*random_range(5,10);

}




//horizontal collision
if (place_meeting(x+hsp, y, obj_enemy_barrier))
{
    while (!place_meeting(x+sign(hsp), y, obj_enemy_barrier))
    {
        x += sign(hsp);
    }
    hsp = -hsp;
}
//vertical collision
if (place_meeting(x, y+vsp, obj_enemy_barrier))
{
    while (!place_meeting(x, y+sign(vsp), obj_enemy_barrier))
    {
        y += sign(vsp);
    }
    vsp = 0;
}

scr_enemy_kill();


x += hsp;
if (vsp &lt; 10) vsp += grav;
y += vsp;

</string>
